	</metadatalist>
	
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="signal">
			<digest>Click trigger</digest>
		</inlet>
		<inlet id="1" type="signal">
			<digest>Settings</digest>
		</inlet>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="signal">
			<digest>clock signal</digest>
		</outlet>
	</outletlist>
	
	<!--ARGUMENTS-->
	<objarglist>
	</objarglist>
	
